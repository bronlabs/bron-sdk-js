{
  "name": "@bronlabs/bron-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Bron API",
  "keywords": [
    "sdk",
    "bron",
    "api",
    "typescript"
  ],
  "author": "Bron Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bronlabs/bron-js-sdk.git"
  },
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm test",
    "dev": "npm run build && node dist/index.js"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "undici": "^6.14.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "^20.10.6",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
