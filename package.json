{
  "name": "@bronlabs/bron-sdk",
  "version": "0.1.32",
  "description": "TypeScript SDK for the Bron API",
  "keywords": [
    "sdk",
    "bron",
    "api",
    "typescript"
  ],
  "author": "Bron Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bronlabs/bron-js-sdk.git"
  },
  "main": "dist/client.js",
  "type": "module",
  "module": "dist/client.js",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "build": "pnpm run update-version && tsc",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "generate-keys": "pnpm run build && node dist/utils/keyGenerator.js",
    "build:generator": "tsc src/utils/generator.ts --outDir dist/utils --module NodeNext --target ES2024 --skipLibCheck",
    "generate": "rm -rf dist && pnpm run build:generator && node dist/utils/generator.js && pnpm run build",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.3",
    "jwk-to-pem": "^2.0.7"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "^24.10.13",
    "cuid": "^3.0.0",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
