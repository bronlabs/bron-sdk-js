{
  "name": "@bronlabs/bron-sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for the Bron API",
  "keywords": [
    "sdk",
    "bron",
    "api",
    "typescript"
  ],
  "author": "Bron Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bronlabs/bron-js-sdk.git"
  },
  "main": "dist/client.js",
  "type": "module",
  "module": "dist/client.js",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "generate-keys": "npm run build && node dist/utils/keyGenerator.js",
    "build:generator": "npx tsc src/utils/generator.ts --outDir dist/utils --module NodeNext --target ES2024 --skipLibCheck",
    "generate": "rm -rf dist && npm run build:generator && node dist/utils/generator.js && npm run build",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "^20.10.6",
    "dotenv": "^17.2.1",
    "cuid": "^3.0.0",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
